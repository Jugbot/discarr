{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "env": ["POSTGRES_URL", "VITE_API_BASE_URL"],
      "inputs": ["$TURBO_DEFAULT$", ".env.local"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "env": ["DISCORD_TOKEN"],
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "env": ["DISCORD_TOKEN"],
      "inputs": ["$TURBO_DEFAULT$", ".env.local"],
      "persistent": true,
      "interactive": false,
      "cache": false
    },
    "prod": {
      "persistent": true,
      "interactive": false,
      "cache": false
    },
    "test": {}
  }
}
